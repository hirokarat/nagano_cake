<h1>新規投稿</h1>
  <%= form_with model:@item, url: '/items', method: :post do |f| %>
<h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
<h4>商品名</h4>
  <%= f.text_field:name %>
<h4>商品説明</h4>
  <%= f.text_area:introduction%>
<h4>税込価格</h4>
  <%= f.text_field:price%>
  <%= f.submit '投稿' %>
  <%end%>
